#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules/terser/bin/node_modules:/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules/terser/node_modules:/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules:/Users/santi/Desktop/Facultad/Ticketera/node_modules:/Users/santi/Desktop/Facultad/node_modules:/Users/santi/Desktop/node_modules:/Users/santi/node_modules:/Users/node_modules:/node_modules:/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules/terser/bin/node_modules:/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules/terser/node_modules:/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules:/Users/santi/Desktop/Facultad/Ticketera/node_modules:/Users/santi/Desktop/Facultad/node_modules:/Users/santi/Desktop/node_modules:/Users/santi/node_modules:/Users/node_modules:/node_modules:/Users/santi/Desktop/Facultad/Ticketera/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
